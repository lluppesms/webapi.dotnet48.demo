<?xml version="1.0"?>
<configuration xmlns:xdt="http://schemas.microsoft.com/XML-Document-Transform">
	<!-- remove these settings and let the Azure App configuration settings take over  -->
	<configBuilders xdt:Transform="Replace">
	</configBuilders>
	<appSettings xdt:Transform="RemoveAttributes(configBuilders)">
	</appSettings>
	<connectionStrings>
		<add name="DatabaseEntities" connectionString="" xdt:Transform="Remove" xdt:Locator="Match(name)"/>
	</connectionStrings>
	<system.web>
		<compilation xdt:Transform="RemoveAttributes(debug)" />
	</system.web>
</configuration>
